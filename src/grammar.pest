substitute = {
    range_regex? ~ "s" ~ PUSH(delim_char) ~ pattern ~ PEEK ~ replacement ~ PEEK ~ flags?
}

range_regex      = { "/" ~ ANY ~ "/" }

delim_char  = { !ASCII_ALPHANUMERIC ~ ANY }
pattern     = { (!PEEK ~ ANY)* }
replacement = { (!PEEK ~ ANY)* }

flags = { flag+ }
flag  = { "g" | "i" | "p" | ASCII_DIGIT+ }

